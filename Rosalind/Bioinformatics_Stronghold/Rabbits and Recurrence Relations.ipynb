    "def rabbits(generations, litter):\n",
    "    \n",
    "    seq = [1]\n",
    "    reproductive = 0\n",
    "    for i in range(generations-1):\n",
    "        seq.append(reproductive + seq[i])\n",
    "        reproductive = seq[i] * litter\n",
    "            \n",
    "    return seq[-1]\n",
    "    "
   ]
